{
    "name": "vinnia/util",
    "license": "MIT",
    "authors": [
        {
            "name": "Joakim Carlsten",
            "email": "joakim@vinnia.se"
        },
        {
            "name": "Johan BjÃ¶rk",
            "email": "johan@vinnia.se"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xmlreader": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Vinnia\\Util\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinnia\\Util\\Tests\\": "tests/unit/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.3"
        },
        "sort-packages": true
    },
    "scripts": {
        "lint": "php-cs-fixer fix --dry-run --diff --diff-format=udiff -vvv --ansi",
        "lint:fix": "php-cs-fixer fix --diff --diff-format=udiff --verbose --ansi",
        "test": "phpunit"
    }
}
